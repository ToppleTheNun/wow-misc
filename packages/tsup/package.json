{
  "name": "@topplethenun/wow-misc-tsup",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "author": "Richard Harrah",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "prepare": "tsup",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@topplethenun/eslint-config-wow-misc": "workspace:*",
    "@topplethenun/wow-misc-tsconfig": "workspace:*",
    "@topplethenun/wow-misc-types": "workspace:*",
    "@types/node": "20.8.6",
    "rimraf": "5.0.5",
    "tsup": "7.2.0",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  },
  "peerDependencies": {
    "tsup": "^7.2.0"
  },
  "engines": {
    "node": ">= 20"
  },
  "publishConfig": {
    "access": "public"
  }
}
