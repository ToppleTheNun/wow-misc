{
  "name": "@topplethenun/wow-misc-monorepo",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ToppleTheNun/wow-misc"
  },
  "scripts": {
    "build": "turbo build",
    "change": "beachball change",
    "checkchange": "beachball check",
    "clean": "turbo clean",
    "dev": "turbo dev --no-cache  --continue",
    "format": "prettier --write packages sims",
    "lint": "turbo lint",
    "release": "beachball publish --message \"[skip ci] applying package updates\"",
    "test": "turbo test",
    "typecheck": "turbo typecheck",
    "update": "turbo update"
  },
  "prettier": "@vercel/style-guide/prettier",
  "devDependencies": {
    "@vercel/style-guide": "5.0.1",
    "beachball": "2.37.0",
    "eslint": "8.48.0",
    "prettier": "3.0.3",
    "turbo": "1.10.15",
    "vitest": "0.34.6"
  },
  "packageManager": "pnpm@8.9.0"
}
